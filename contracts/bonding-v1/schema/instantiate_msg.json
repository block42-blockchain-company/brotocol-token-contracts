    "bro_token": {
      "type": "string"
    },
    "lp_bonding_discount": {
      "$ref": "#/definitions/Decimal"
    },
    "lp_token": {
      "type": "string"
    },
    "owner": {
      "type": "string"
    },
    "treasury_contract": {
