{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Cw20HookMsg",
  "description": "This structure describes the cw20 receive hook messages of the contract.",
  "oneOf": [
    {
      "title": "Description",
      "description": "Registers sale and whitelists addresses",
      "type": "object",
      "required": [
        "register_sale"
      ],
      "properties": {
        "register_sale": {
          "type": "object",
          "required": [
            "accounts",
            "sale_end_time",
            "sale_start_time"
          ],
          "properties": {
            "accounts": {
              "description": "whitelisted accounts",
              "allOf": [
                {
                  "$ref": "#/definitions/Binary"
                }
              ]
            },
            "sale_end_time": {
              "description": "sale end time",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            },
            "sale_start_time": {
              "description": "sale start time",
              "type": "integer",
              "format": "uint64",
              "minimum": 0.0
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "Binary": {
      "description": "Binary is a wrapper around Vec<u8> to add base64 de/serialization with serde. It also adds some helper methods to help encode inline.\n\nThis is only needed as serde-json-{core,wasm} has a horrible encoding for Vec<u8>",
      "type": "string"
    }
  }
}
